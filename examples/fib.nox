import { min } from math


func fib(n: Int): Int {
    if (n <= 1) {
        return min(n, 0)
    }

    return fib(n - 1) + fib(n - 2)
}


//
// OR
//

func fib(n: Int): Int {
    return (n)
        | n if n <= 0 -> 0
        | 1 -> 1
        | _ -> fib(n - 1) + fib(n - 2)
}


//
// OR
//

func fib(n: Int): Int {
    return
        | n <= 0  -> 0
        | n == 1  -> 1

    return fib(n - 1) + fib(n - 2)
}
