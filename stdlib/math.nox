import { foldl } from core
import { ceil, floor } from _internal
import operator from operator


export ceil


export floor


smallest =
    (x, y) => x < y ? x : y


largest =
    (x, y) => x > y ? x : y


export min = foldl(?, smallest)


export max = foldl(?, largest)


export sum = foldl(?, operator.add, 0)
